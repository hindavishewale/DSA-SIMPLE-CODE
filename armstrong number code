#include <iostream>
using namespace std;
int main() 
{
    int number,rem,sum=0;
    cout<<"Enter no: ";
    cin>>number;
    int num=number;
    while(number!=0)
    {
        rem=number%10;
        rem=rem*rem*rem;
        sum=sum+rem;
        number/=10;
    }
    if(sum==num)
    {
        cout<<"Number is armstrong ";
    }
    else
    {
        cout<<"Not!!";
    }

    return 0;
}
